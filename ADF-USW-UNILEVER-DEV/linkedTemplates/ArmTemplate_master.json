{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADF-USW-UNILEVER-DEV"
		},
		"LS_AzureSqlSourceDB_ZBBDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_AzureSqlSourceDB_ZBBDB'"
		},
		"LS_AzureSqlTargetDB_ZBBDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_AzureSqlTargetDB_ZBBDB'"
		},
		"LS_SQL_cargo_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_SQL_cargo'"
		},
		"LS_SQL_cargo_source_load_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_SQL_cargo_source_load'"
		},
		"LS_blobstorage_Cargeo_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_blobstorage_Cargeo'"
		},
		"LS_blobstorage_zbbstorageaccount_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_blobstorage_zbbstorageaccount'"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADF-USW-UNILEVER-DEV_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_AzureSqlSourceDB_ZBBDB_connectionString": {
						"value": "[parameters('LS_AzureSqlSourceDB_ZBBDB_connectionString')]"
					},
					"LS_AzureSqlTargetDB_ZBBDB_connectionString": {
						"value": "[parameters('LS_AzureSqlTargetDB_ZBBDB_connectionString')]"
					},
					"LS_SQL_cargo_connectionString": {
						"value": "[parameters('LS_SQL_cargo_connectionString')]"
					},
					"LS_SQL_cargo_source_load_connectionString": {
						"value": "[parameters('LS_SQL_cargo_source_load_connectionString')]"
					},
					"LS_blobstorage_Cargeo_connectionString": {
						"value": "[parameters('LS_blobstorage_Cargeo_connectionString')]"
					},
					"LS_blobstorage_zbbstorageaccount_connectionString": {
						"value": "[parameters('LS_blobstorage_zbbstorageaccount_connectionString')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "ADF-USW-UNILEVER-DEV_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ADF-USW-UNILEVER-DEV_ArmTemplate_0')]"
			]
		},
		{
			"name": "ADF-USW-UNILEVER-DEV_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ADF-USW-UNILEVER-DEV_ArmTemplate_1')]"
			]
		}
	]
}